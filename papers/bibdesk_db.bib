	Doi = {10.1051/0004-6361/200913396},
	Eid = {A27},
	Eprint = {1006.4702},
	Journal = {\aap},
	Keywords = {planetary systems, stars: atmospheres, planets and satellites: general},
	Month = sep,
	Pages = {A27},
