	Date-Added = {2013-05-06 23:47:27 +0000},
	Date-Modified = {2013-05-06 23:47:27 +0000},
	Doi = {10.1051/0004-6361:20079321},
	Eprint = {0802.1810},
	Journal = {\aap},
	Keywords = {stars: planetary systems, stars: individual: GJ 436, stars: individual: Hat-P-2, stars: individual: HD 149026},
	Month = apr,
