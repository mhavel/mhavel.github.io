	Eprint = {1312.3327},
	Journal = {International Journal of Astrobiology},
	Keywords = {Exoplanets, Tidal Theory, Transits,},
	Month = apr,
	Pages = {321-333},
	Primaryclass = {astro-ph.EP},
	Title = {{A method to identify the boundary between rocky and gaseous exoplanets from tidal theory and transit durations}},
