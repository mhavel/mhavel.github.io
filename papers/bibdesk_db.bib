	Eprint = {1406.4127},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Earth and Planetary Astrophysics},
	Month = jun,
	Primaryclass = {astro-ph.EP},
	Title = {{Grain opacity and the bulk composition of extrasolar planets. II. An analytical model for the grain opacity in protoplanetary atmospheres}},
