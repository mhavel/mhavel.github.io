	Doi = {10.1111/j.1365-2966.2009.14868.x},
	Eprint = {0812.1463},
	Journal = {\mnras},
	Keywords = {planetary systems},
	Month = jul,
	Pages = {1789-1796},
	Title = {{Empirical evidence for tidal evolution in transiting planetary systems}},
	Volume = 396,
	Year = 2009,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2009.14868.x},
