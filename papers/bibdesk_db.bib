	Doi = {10.1086/307387},
	Journal = {\apj},
	Keywords = {MOLECULAR PROCESSES, STARS: ABUNDANCES, STARS: INDIVIDUAL (GLIESE 229B), STARS: LOW-MASS, BROWN DWARFS, Molecular Processes, Stars: Abundances, stars: individual (Gliese 229B), Stars: Low-Mass, Brown Dwarfs},
	Month = jul,
	Pages = {793-801},
	Title = {{Alkali Element Chemistry in Cool Dwarf Atmospheres}},
	Volume = 519,
	Year = 1999,
	Bdsk-File-1 = {},
