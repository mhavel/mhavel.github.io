	Keywords = {celestial mechanics, planets and satellites: atmospheres, planets and satellites: individual: CoRoT-7 b},
	Month = sep,
	Pages = {910-922},
	Primaryclass = {astro-ph.EP},
	Title = {{The roles of tidal evolution and evaporative mass loss in the origin of CoRoT-7 b}},
	Volume = 407,
