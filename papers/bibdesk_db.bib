	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Earth and Planetary Astrophysics},
	Month = jul,
	Primaryclass = {astro-ph.EP},
	Title = {{BEER analysis of Kepler and CoRoT light curves: II. Evidence for emission phase shift due to superrotation in four Kepler hot Jupiters}},
	Year = 2014,
	Bdsk-File-1 = {},
