@article{Majeau:2012a,
	Abstract = {After the publication of this Letter, we discovered an important north-south degeneracy in the eclipse mapping technique. The planet's orbit is not perfectly edge-on, and we do not know whether its orbital angular momentum vector is tilted toward or away from us. We therefore do not know whether the planet's northern or southern regions are occulted first during ingress. Accounting for this degeneracy slightly strengthens our conclusion that the planet's hottest latitude is equatorial. We now find a hot spot latitude within 21fdg1 of the equator for the secondary eclipse inversion, albeit with a slight preference for off-equatorial locations, and within 10fdg1 of the equator for the eclipse plus phase-function inversion (68% confidence). Furthermore, one of the original figure captions confused ingress and egress. Following the standard convention that a planet's rotational angular momentum points north, the western regions of the planet are occulted first during ingress, and revealed first during egress.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2012ApJ...757L..32M},
	Author = {{Majeau}, C. and {Agol}, E. and {Cowan}, N.~B.},
