	Keywords = {techniques: spectroscopic, stars: abundances, circumstellar matter, stars: late-type, planetary systems},
	Month = may,
	Pages = {A40},
	Primaryclass = {astro-ph.EP},
	Title = {{Metallicity of solar-type stars with debris discs and planets{\star}}},
	Volume = 541,
	Year = 2012,
