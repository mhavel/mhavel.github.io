	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1601.05107},
	Author = {{Figueira}, P. and {Faria}, J.~P. and {Adibekyan}, V.~Z. and {Oshagh}, M. and {Santos}, N.~C.},
	Date-Added = {2016-01-31 00:17:48 +0000},
	Date-Modified = {2016-01-31 00:17:48 +0000},
	Eprint = {1601.05107},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Instrumentation and Methods for Astrophysics, Astrophysics - Earth and Planetary Astrophysics},
	Month = jan,
