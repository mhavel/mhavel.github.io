	Journal = {\apj},
	Keywords = {{magnetohydrodynamics: MHD, planets and satellites: atmospheres}},
	Month = {f{\'e}vrier},
	Pages = {{138}},
	Title = {"{Magnetic Scaling Laws for the Atmospheres of Hot Giant Exoplanets}"},
	Volume = {745},
