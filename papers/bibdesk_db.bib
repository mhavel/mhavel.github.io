	Doi = {10.1002/2014JA020019},
	Eprint = {1502.05504},
	Journal = {Journal of Geophysical Research (Space Physics)},
	Keywords = {magnetosheath, magnetic field draping, Saturn},
	Month = jul,
	Pages = {5651-5661},
	Primaryclass = {astro-ph.EP},
