	Date-Added = {2013-05-06 23:53:59 +0000},
	Date-Modified = {2013-05-06 23:53:59 +0000},
	Doi = {10.1016/j.icarus.2010.11.004},
	Eprint = {1011.1713},
	Journal = {\icarus},
	Month = feb,
	Pages = {1258-1273},
	Primaryclass = {astro-ph.EP},
	Title = {{Scaling laws for convection and jet speeds in the giant planets}},
	Volume = 211,
	Year = 2011,
