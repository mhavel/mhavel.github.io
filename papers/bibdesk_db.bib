	Author = {{Maxted}, P.~F.~L. and {Serenelli}, A.~M. and {Southworth}, J.},
	Date-Added = {2015-04-13 18:30:16 +0000},
	Date-Modified = {2015-04-13 18:30:54 +0000},
	Eprint = {1503.09111},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Solar and Stellar Astrophysics},
	Month = mar,
	Primaryclass = {astro-ph.EP},
	Read = {1},
	Title = {{A comparison of gyrochronological and isochronal age estimates for transiting exoplanet host stars}},
