	Month = jan,
	Pages = {25},
	Primaryclass = {astro-ph.EP},
	Title = {{Chemical Consequences of the C/O Ratio on Hot Jupiters: Examples from WASP-12b, CoRoT-2b, XO-1b, and HD 189733b}},
