	Author = {{Mordasini}, C. and {Alibert}, Y. and {Klahr}, H. and {Henning}, T.},
	Date-Added = {2013-07-27 21:50:30 +0000},
	Date-Modified = {2013-07-27 21:50:30 +0000},
	Doi = {10.1051/0004-6361/201118457},
	Eid = {A111},
	Eprint = {1206.6103},
	Journal = {\aap},
	Keywords = {planetary systems, planet-disk interactions, planets and satellites: formation, planets and satellites: interiors, planets and satellites: individual: Jupiter, methods: numerical},
	Month = nov,
