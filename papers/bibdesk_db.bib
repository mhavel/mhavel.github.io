	Eprint = {0902.3995},
	Journal = {\apj},
	Keywords = {astrochemistry, diffusion, planetary systems, radiative transfer, turbulence},
	Month = jul,
	Pages = {1487-1500},
	Primaryclass = {astro-ph.EP},
	Title = {{Can TiO Explain Thermal Inversions in the Upper Atmospheres of Irradiated Giant Planets?}},
