	Author = {{Perez-Becker}, D. and {Showman}, A.~P.},
	Date-Added = {2013-07-08 18:26:48 +0000},
	Date-Modified = {2013-07-08 18:26:48 +0000},
	Eprint = {1306.4673},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Earth and Planetary Astrophysics},
	Month = jun,
	Primaryclass = {astro-ph.EP},
	Title = {{Atmospheric Heat Redistribution on Hot Jupiters}},
	Year = 2013,
	Bdsk-File-1 = {},
