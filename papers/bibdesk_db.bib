	Adsurl = {http://adsabs.harvard.edu/abs/2013A%26A...554A..84M},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1303.3418},
	Author = {{Maldonado}, J. and {Villaver}, E. and {Eiroa}, C.},
	Date-Added = {2016-01-12 22:55:03 +0000},
	Date-Modified = {2016-01-12 22:55:04 +0000},
	Doi = {10.1051/0004-6361/201321082},
	Eid = {A84},
	Eprint = {1303.3418},
	Journal = {\aap},
	Keywords = {techniques: spectroscopic, stars: abundances, stars: late-type, planetary systems},
	Month = jun,
	Pages = {A84},
	Primaryclass = {astro-ph.EP},
	Title = {{The metallicity signature of evolved stars with planets}},
	Volume = 554,
	Year = 2013,
