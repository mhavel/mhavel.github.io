	Pages = {L32},
	Title = {{ERRATUM: ''A Two-dimensional Map of the Extrasolar Planet HD 189733b'' <A href=''/abs/2012ApJ...747L..20M''>(2012, ApJ, 747, L20)</A>}},
	Volume = 757,
