	Journal = {\aaps},
	Keywords = {METHODS: NUMERICAL, PLANETS: INDIVIDUAL, JUPITER, NEPTUNE, SATURN, URANUS},
	Month = jan,
	Pages = {109-123},
	Title = {{CEPAM: a code for modeling the interiors of giant planets.}},
