	Editor = {{Buccheri}, R. and {van Paradijs}, J. and {Alpar}, A.},
	Eprint = {arXiv:astro-ph/9709094},
	Pages = {419},
	Title = {{Thermonuclear Burning on Rapidly Accreting Neutron Stars}},
	Year = 1998,
	Bdsk-File-1 = {},
