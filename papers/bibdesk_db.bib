	Eprint = {1407.4150},
	Journal = {Annual Review of Earth and Planetary Sciences},
	Keywords = {Astrophysics - Earth and Planetary Astrophysics},
	Month = may,
	Pages = {509-540},
	Primaryclass = {astro-ph.EP},
	Title = {{Atmospheric Dynamics of Hot Exoplanets}},
	Volume = 43,
	Year = 2015,
	Bdsk-File-1 = {},
