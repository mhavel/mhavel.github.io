	Journal = {\apj},
	Keywords = {accretion, accretion disks, methods: analytical, planet-disk interactions, planets and satellites: formation, protoplanetary disks, turbulence},
	Month = jun,
	Pages = {62},
	Primaryclass = {astro-ph.EP},
	Title = {{Planet Traps and First Planets: The Critical Metallicity for Gas Giant Formation}},
