	Date-Added = {2014-07-18 12:15:51 +0000},
	Date-Modified = {2014-07-18 12:15:51 +0000},
	Eprint = {1406.0652},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Earth and Planetary Astrophysics},
	Month = jun,
	Primaryclass = {astro-ph.SR},
	Title = {{Asteroseismology for ''$\backslash$`$\{$a$\}$ la carte'' stellar age-dating and weighing: Age and mass of the CoRoT exoplanet host HD 52265}},
