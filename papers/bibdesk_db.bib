	Date-Modified = {2013-07-27 22:28:03 +0000},
	Doi = {10.1111/j.1365-2966.2011.19399.x},
	Eprint = {1107.1235},
	Journal = {\mnras},
	Keywords = {binaries: eclipsing , stars: fundamental parameters , planetary systems},
	Month = {novembre},
	Pages = {2166-2196},
	Title = {{Homogeneous studies of transiting extrasolar planets - IV. Thirty systems with space-based light curves}},
