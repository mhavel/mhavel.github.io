	Doi = {10.1051/0004-6361/201116451},
	Eid = {A75},
	Journal = {\aap},
	Keywords = {binaries: eclipsing, stars: interiors, stars: rotation, stars: atmospheres, planetary systems},
	Month = may,
	Pages = {A75},
	Title = {{Gravity and limb-darkening coefficients for the Kepler, CoRoT, Spitzer, uvby, UBVRIJHK, and Sloan photometric systems}},
	Volume = 529,
	Year = 2011,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1051/0004-6361/201116451},
