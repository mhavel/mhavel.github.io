	Date-Added = {2016-01-12 22:37:54 +0000},
	Date-Modified = {2016-01-12 22:37:55 +0000},
	Doi = {10.1051/0004-6361/201525774},
	Eid = {A90},
	Eprint = {1503.09111},
	Journal = {\aap},
	Keywords = {stars: solar-type, planet-star interactions},
	Month = may,
	Pages = {A90},
	Primaryclass = {astro-ph.EP},
	Title = {{Comparison of gyrochronological and isochronal age estimates for transiting exoplanet host stars}},
