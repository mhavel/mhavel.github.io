@inproceedings{Torres:2009,
	Abstract = {With 40 or more transiting exoplanets now known, the time is ripe to seek patterns and correlations among their observed properties, which may give important insights into planet formation, structure, and evolution. This task is made difficult by the widely different methodologies that have been applied to measure their properties in individual cases. Furthermore, in many systems our knowledge of the planet properties is limited by the knowledge of the properties of the parent stars. To address these difficulties we have undertaken the first comprehensive analysis of the data for 23 transiting planets using a uniform methodology. We revisit several of the recently proposed correlations, and find new ones involving the metallicity of the parent stars.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2009IAUS..253..482T},
