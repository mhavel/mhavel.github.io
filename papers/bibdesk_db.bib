	Author = {{Verner}, G.~A. and {Elsworth}, Y. and {Chaplin}, W.~J. and {Campante}, T.~L. and {Corsaro}, E. and {Gaulme}, P. and {Hekker}, S. and {Huber}, D. and {Karoff}, C. and {Mathur}, S. and {Mosser}, B. and {Appourchaux}, T. and {Ballot}, J. and {Bedding}, T.~R. and {Bonanno}, A. and {Broomhall}, A.-M. and {Garc{\'{\i}}a}, R.~A. and {Handberg}, R. and {New}, R. and {Stello}, D. and {R{\'e}gulo}, C. and {Roxburgh}, I.~W. and {Salabert}, D. and {White}, T.~R. and {Caldwell}, D.~A. and {Christiansen}, J.~L. and {Fanelli}, M.~N.},
