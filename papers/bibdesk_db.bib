	Arxivurl = {http://arXiv.org/abs/astro-ph/0607212},
	Author = {{Ikoma}, M. and {Guillot}, T. and {Genda}, H. and {Tanigawa}, T. and {Ida}, S.},
	Date-Added = {2013-05-07 00:33:59 +0000},
