	Journal = {\mnras},
	Keywords = {methods: numerical, planets and satellites: atmospheres},
	Month = dec,
	Pages = {2669-2696},
	Primaryclass = {astro-ph.EP},
	Title = {{Atmospheric circulation of tidally locked exoplanets: II. Dual-band radiative transfer and convective adjustment}},
	Volume = 418,
