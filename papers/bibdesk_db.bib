	Doi = {10.1111/j.1365-2966.2007.12512.x},
	Eprint = {arXiv:astro-ph/0703766},
	Journal = {\mnras},
	Keywords = {stars: abundances, stars: fundamental parameters (colours, luminosities, masses, radii, temperatures, etc.), Hertzsprung-Russell (HR) diagram, binaries: general, stars: interiors, stars: late-type},
	Month = dec,
	Pages = {1516-1540},
